V1 = 8; 
V2 = 11; 
n = 3;     
k = (V2 / V1) - 1; 
V3 = V2+(V1+V2)*k;   
V_total = V1 + V2 + V3;   
V_line = 1.732*V_total;  
string_efficiency = (V_total/(n*V3))*100;  
disp('Ratio of capacitance between pin and earth (C0) to self-capacitance (C)=',k); 
disp('Line Voltage (kV)=',V_line); 
disp('String Efficiency (%)=',string_efficiency);
